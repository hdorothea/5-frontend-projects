{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-else-return": 0,
    "consistent-return": 0,
    "no-continue": 0,
    "no-restricted-syntax": 0
  },
  "globals": {
    "window": true,
    "document": true
  },
  "env": {
    "jasmine": true,
    "node": true
  }
}